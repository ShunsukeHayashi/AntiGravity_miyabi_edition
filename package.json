{
  "name": "antigravity-ide",
  "displayName": "AntiGravity IDE",
  "version": "0.1.0",
  "description": "AI-powered IDE with agent-first architecture, inspired by Google AntiGravity",
  "main": "./out/main.js",
  "author": "Shunsuke Hayashi",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:ShunsukeHayashi/AntiGravity_miyabi_edition.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "build": "tsc -p ./",
    "build:extension": "cd extensions/antigravity-core && npm run compile",
    "build:vscode": "cd vscode-upstream && yarn compile",
    "build:all": "npm run build && npm run build:extension",
    "watch": "tsc -watch -p ./",
    "watch:extension": "cd extensions/antigravity-core && npm run watch",
    "compile": "npm run build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint src --ext ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "pretest": "npm run build && npm run lint",
    "package": "npm run build:all && electron-builder",
    "dev": "electron ./out/main.js",
    "dev:extension": "code --extensionDevelopmentPath=./extensions/antigravity-core",
    "install:extension": "cd extensions/antigravity-core && npm install",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.24.2",
    "@octokit/rest": "^22.0.1",
    "@tanstack/react-query": "^5.17.0",
    "clsx": "^2.1.0",
    "electron": "^28.0.0",
    "framer-motion": "^11.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.25",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "autoprefixer": "^10.4.16",
    "electron-builder": "^24.9.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "build": {
    "appId": "com.antigravity.ide",
    "productName": "AntiGravity IDE",
    "directories": {
      "output": "dist"
    },
    "files": [
      "out/**/*",
      "resources/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "resources/icon.icns",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "icon": "resources/icon.ico",
      "target": [
        "nsis",
        "zip"
      ]
    },
    "linux": {
      "icon": "resources/icon.png",
      "target": [
        "AppImage",
        "deb",
        "rpm"
      ],
      "category": "Development"
    }
  }
}
